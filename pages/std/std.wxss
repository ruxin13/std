}
.theme.t1 .theme-cir{
  background-image: linear-gradient(-45deg, rgba(255,142,100,1) 0%, rgba(255,201,77,1) 100%);
}
.theme.t2 .theme-cir{
  background-image: linear-gradient(225deg, rgba(255,137,96,1) 0%, rgba(255,98,165,1) 100%);
}
.theme.t3 .theme-cir{
  background-image: linear-gradient(-43deg, rgba(91,165,255,1) 0%, rgba(77,219,255,1) 83%, rgba(91,220,235,.2) 100%);
}
.theme.t4 .theme-cir{
  background-image: linear-gradient(135deg, rgba(174,239,170,1) 6%, rgba(59,214,161,1) 100%);
}
.theme.cur .theme-bgcir{
  transform: scale(1.15);
  opacity: 1;
  transition: transform .3s ease;
}
.theme.cur.t1 .theme-bgcir{
  background-image: linear-gradient(-45deg, rgba(255,142,100,.2) 0%, rgba(255,201,77,.2) 100%);
}
.theme.cur.t2 .theme-bgcir{
