.std-svg{
  width: 600rpx;
  height: 600rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate3d(-50%, -50%, 0);
}
.an-svg1{
  animation: round 24s ease-in-out infinite;
}
.an-svg2{
  animation: round 20s ease-in-out infinite 5s;
}
.an-svg3{
  animation: round 18s ease-in-out infinite 9s;
}
@keyframes round {
  0%{
    transform: translate3d(-50%, -50%, 0) rotate(0deg);
  }

  10%{ opacity: 1;}
  15%{ opacity: 0;}
  20%{ opacity: 1;}

  35%{ opacity: 1;}
  40%{ opacity: 0;}
  45%{ opacity: 1;}

  60%{ opacity: 1;}
  65%{ opacity: 0;}
  70%{ opacity: 1;}

  85%{ opacity: 1;}
  90%{ opacity: 0;}
  95%{ opacity: 1;}

  100%{
    transform: translate3d(-50%, -50%, 0) rotate(360deg);
  }
}


.std-bga{
  width: 360rpx;
  height: 360rpx;
  border-radius: 50%;
  opacity: 1;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate3d(-50%, -50%, 0);
  z-index: 2;
}
.std-bga.theme1{
  background-image: linear-gradient(-45deg, rgba(255,142,100,.2) 0%, rgba(255,201,77,.2) 100%);
}
.std-bga.theme2{
  background-image: linear-gradient(225deg, rgba(255,137,96,.2) 0%, rgba(255,98,165,.2) 100%);
}
.std-bga.theme3{
  background-image: linear-gradient(-43deg, rgba(91,165,255,.2) 0%, rgba(77,219,255,.2) 83%, rgba(91,220,235,.2) 100%);
}
.std-bga.theme4{
  background-image: linear-gradient(243deg, rgba(38,219,196,.2) 6%, rgba(59,214,161,.2) 100%);
}
.an-wave{
  animation: wave 1s ease;
}
@keyframes wave {
  0%{
    opacity: 1;
  }
  100%{
    transform: translate3d(-50%, -50%, 0) scale(1.5);
    opacity: 0;
  }
}
