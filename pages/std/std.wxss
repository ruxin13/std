}
.theme.cur.t3 .theme-bgcir{
  background-image: linear-gradient(-43deg, rgba(91,165,255,.2) 0%, rgba(77,219,255,.2) 83%, rgba(91,220,235,.2) 100%);
}
.theme.cur.t4 .theme-bgcir{
  background-image: linear-gradient(243deg, rgba(38,219,196,.2) 6%, rgba(59,214,161,.2) 100%);
}
.theme.cur .theme-cir{
  opacity: 1;
}
